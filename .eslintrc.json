{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-undef":"off",
        "accessor-pairs": "off",
        "no-async-promise-executor": "off",
        "prefer-promise-reject-errors": "off"
    }
}
